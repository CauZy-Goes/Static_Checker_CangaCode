// Exemplo para testar repetição de identificadores

program SistemaCadastro

declarations
    integer idade
    real altura
    string nome
    boolean ativo
endDeclarations

functions
funcType calcularIMC
    real peso
    real altura
endFuncType

funcType exibirUsuario
    string nome
    integer idade
    boolean ativo
endFuncType
endFunctions

start
    idade = 30
    altura = 1.82
    nome = "Carlos"
    ativo = true

    calcularIMC(80.5, altura)
    exibirUsuario(nome, idade, ativo)
endProgram
